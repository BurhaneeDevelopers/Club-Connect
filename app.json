{
  "expo": {
    "name": "Vibehotspot",
    "slug": "vibehotspot",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splash-final.png",
      "resizeMode": "cover",
      "backgroundColor": "#101010"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": ".vibehotspot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#101010"
      },
      "googleServicesFile": "google-services.json",
      "package": "com.taheri_dev72.vibehotspot",
      "UIBackgroundModes": [
        "audio"
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a7079c16-20a2-4dfc-b6ea-918c9d2380fa"
      }
    },
    "owner": "taheri_dev72",
    "updates": {
      "url": "https://u.expo.dev/a7079c16-20a2-4dfc-b6ea-918c9d2380fa"
    },
    "notification": {
      "icon": "./assets/adaptive-icon.png",
      "color": "#101010",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "VibeHotspot"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
